{
  "buildCommand": "cd apps/web && npm install && npm run build",
  "outputDirectory": "apps/web/.next", 
  "framework": "nextjs",
  "installCommand": "cd apps/web && npm install",
  "functions": {
    "apps/web/app/api/chat/sse/route.ts": {
      "maxDuration": 10
    },
    "apps/web/app/api/sse/master/route.ts": {
      "maxDuration": 10
    },
    "apps/web/app/api/sse/network/route.ts": {
      "maxDuration": 10
    }
  },
  "env": {
    "SOVEREIGNTY_ENFORCED": "true",
    "TARGET_COST": "0.001",
    "NEXT_PUBLIC_APP_URL": "https://exprezzzo-house.vercel.app"
  }
}
