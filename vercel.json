{
  "buildCommand": "cd apps/web && npm install && npm run build",
  "outputDirectory": "apps/web/.next",
  "installCommand": "npm install",
  "framework": "nextjs",
  "env": {
    "NODE_ENV": "production",
    "SOVEREIGNTY_MODE": "enforced",
    "MAX_COST_PER_REQUEST": "0.001",
    "BRAND_GOLD": "#C5B358",
    "BRAND_CHOCOLATE": "#381819",
    "BRAND_SAND": "#EDC9AF",
    "BRAND_ROSE": "#C72C41",
    "BRAND_DUST": "#A89F91",
    "BRAND_LIGHT_SAND": "#F5F5DC"
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://exprezzzo-api.vercel.app/api/:path*"
    }
  ]
}